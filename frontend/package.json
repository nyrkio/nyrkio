{
  "name": "frontend",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "proxy": "http://localhost:80",
  "devServer": {
    "proxy": {
      "*": "http://[::1]:80"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier . --write",
    "preview": "vite preview",
    "playwright": "./node_modules/playwright/cli.js",
    "test": "npx playwright test",
    "test:unit": "npx playwright test tests/*.spec.ts",
    "test:integration": "npx playwright test --config=playwright.config.integration.ts",
    "test-ui": "npx playwright test --ui",
    "test-ui:integration": "npx playwright test --config=playwright.config.integration.ts --ui",
    "test-report": "npx playwright show-report",
    "test-report:integration": "npx playwright show-report test-results/integration-report"
  },
  "dependencies": {
    "ag-grid-react": "^31.0.1",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^4.4.1",
    "chartjs-plugin-zoom": "^2.2.0",
    "current-git-branch": "^1.1.0",
    "date-fns": "^3.0.6",
    "posthog-js": "^1.103.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha-v3": "^1.11.0",
    "react-markdown": "^9.0.1",
    "react-native-rss-parser": "^1.5.1",
    "react-router-dom": "^6.21.0",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "bcrypt": "^6.0.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "git-commit-info": "^2.0.2",
    "mongodb": "^7.0.0",
    "playwright": "^1.42.1",
    "prettier": "3.2.5",
    "vite": "^5.0.13"
  }
}
